// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  id String @id @default(uuid())
  provider String
  thirdPartyId String
  email String?
  displayName String?
  picture String?
  todos Todo[]
}

model Todo {
  id String @id
  text String
  ddl DateTime
  complete Boolean
  priority String
  user User @relation(fields: [userId], references: [id])
  userId String
}

model Cat {
  id Int @id @default(autoincrement())
  name String
  age Int
}